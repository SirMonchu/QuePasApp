<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="245.0" prefWidth="328.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.QuePasApp.controllers.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Text fill="#3ec679" strokeType="OUTSIDE" strokeWidth="0.0" text="UserName">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
   
       <Label fx:id="welcomeText" />
      <TextField fx:id="UserNAmetxt" />
      <Button fx:id="SubmitButton" mnemonicParsing="false" onAction="#onHelloButtonClick" prefHeight="46.0" prefWidth="88.0" style="-fx-background-color: #008000;" text="Submit" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</VBox>
